    "enzyme-to-json": "^3.3.4",
    "f1-telemetry-client": "0.1.4",
    "file-loader": "^2.0.0",
    "gts": "^0.9.0",
    "husky": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
